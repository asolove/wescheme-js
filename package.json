{
  "name": "wescheme-js",
  "version": "0.0.1",
  "description": "wescheme javascript compiler",
  "main": "lib/wescheme.js",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "codemirror": "^5.8.0",
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "jest-cli": "^0.8.0",
    "pretty-js": "^0.1.8",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "scripts": {
    "compile": "webpack --config webpack-package.config.js && ./node_modules/babel-cli/bin/babel.js -D -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "jest",
    "test-coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack-dev-server --content-base example/",
    "start-test": "webpack-dev-server --port 8085 --content-base test/",
    "lint": "eslint . --ext .js || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcardune/wescheme-js.git"
  },
  "author": "",
  "license": "LGPL",
  "bugs": {
    "url": "https://github.com/pcardune/wescheme-js/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "src/analyzer.js": false,
      "src/compiler.js": false,
      "src/lex.js": false,
      "src/modules.js": false,
      "src/parser.js": false,
      "TODO: src/structures.js bug in instanbul makes this blow up?": false
    }
  },
  "homepage": "https://github.com/pcardune/wescheme-js"
}
